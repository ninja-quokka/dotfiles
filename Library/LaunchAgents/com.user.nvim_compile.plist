<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.user.nvim_compile</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/ldenny/.local/bin/vv</string>
        <string>+q</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <false/>

    <key>StandardOutPath</key>
    <string>/tmp/nvim_compile-out.log</string>

    <key>StandardErrorPath</key>
    <string>/tmp/nvim_compile-err.log</string>
</dict>
</plist>
